msgctxt "#31400"
msgid "[B]CONFIGURE APPEARANCE SETTINGS[/B][CR][CR][COLOR=midgrey]Change the skin · Set language and region · Change file listing options · Set up a screensaver[/COLOR]"
msgstr "[B]CONFIGUREER UITERLIJKE INSTELLINGEN[/B][CR][CR][COLOR=midgrey] Verander skin · Verander taal en regio  · Verander bestandsindeling opties · Stel een screensaver in[/COLOR]"

msgctxt "#31401"
msgid "[B]CONFIGURE VIDEO SETTINGS[/B][CR][CR][COLOR=midgrey]Manage your video library · Set video playback options · Change video listing options · Set subtitle fonts[/COLOR]"
msgstr "[B]CONFIGUREER VIDEO INSTELLINGEN[/B][CR][CR][COLOR=midgrey] Beheer uw video bibliotheek · Stel video afspeel opties in  · Stel videoindeling opties in · Wijzig het ondertitel lettertype[/COLOR]"

msgctxt "#31402"
msgid "[B]CONFIGURE MUSIC SETTINGS[/B][CR][CR][COLOR=midgrey]Manage your music library · Set music playback options · Change music listing options · Setup song submission · Set karaoke options[/COLOR]"
msgstr "[B]CONFIGUREER MUZIEK INSTELLINGEN[/B][CR][CR][COLOR=midgrey]Beheer uw muziek bibliotheek · Stel muziek afspeel opties in · Stel de muziek indeling in · Stel de nummer submissie in · Stel de karaoke opties in[/COLOR]"

msgctxt "#31403"
msgid "[B]CONFIGURE PICTURE SETTINGS[/B][CR][CR][COLOR=midgrey]Set picture listing options · Configure slideshows[/COLOR]"
msgstr "[B]CONFIGUREER AFBEELDING OPTIES[/B][CR][CR][COLOR=midgrey]Verander afbeeldingsindeling opties · Configureer diavoorstelling[/COLOR]"

msgctxt "#31404"
msgid "[B]CONFIGURE WEATHER SETTINGS[/B][CR][CR][COLOR=midgrey]Set three cities to collect weather information[/COLOR]"
msgstr "[B]CONFIGUREER WEER INSTELLINGEN[/B][CR][CR][COLOR=midgrey]Stel drie steden in om weer informatie van te ontvangen[/COLOR]"

msgctxt "#31405"
msgid "[B]CONFIGURE NETWORK SETTINGS[/B][CR][CR][COLOR=midgrey]Setup control of XBMC via UPnP and HTTP · Configure file sharing · Set internet access options[/COLOR]"
msgstr "[B]CONFIGUREER NETWERK INSTELLINGEN[/B][CR][CR][COLOR=midgrey]Stel de besturing van xbmc via UPnP en HTTP in · Configureer bestands deling · Stel internet toegang opties in[/COLOR]"

msgctxt "#31406"
msgid "[B]CONFIGURE SYSTEM SETTINGS[/B][CR][CR][COLOR=midgrey]Setup and calibrate displays · Configure audio output · Setup remote controls · Set power saving options · Enable debugging · Setup master lock[/COLOR]"
msgstr "[B]CONFIGUREER SYSTEEM INSTELLINGEN[/B][CR][CR][COLOR=midgrey]Installeer en kalibreer schermen · Configureer audio uitgang · Stel de afstandsbedieningen in · Stel de stroom besparings opties in · Schakel debugging in · Installeer master lock[/COLOR]"

msgctxt "#31407"
msgid "[B]CONFIGURE SKIN SETTINGS[/B][CR][CR][COLOR=midgrey]Setup the Avalon skin · Add and remove home menu items · Change skin backgrounds[/COLOR]"
msgstr "[B]CONFIGUREER SKIN INSTELLINGEN[/B][CR][CR][COLOR=midgrey]Stel de Titan skin in · Voeg toe en verwijder home menu items · Verander skin achtergronden[/COLOR]"

msgctxt "#31408"
msgid "[B]CONFIGURE ADD-ONS[/B][CR][CR][COLOR=midgrey]Manage your installed Add-ons · Browse for and install Add-ons from xbmc.org · Modify Add-on settings[/COLOR]"
msgstr "[B]CONFIGUREER ADD-ONS[/B][CR][CR][COLOR=midgrey]Beheer uw geïnstalleerde Add-ons · Blader en installeer Add-ons van xbmc.org · Wijzig Add-on instellingen[/COLOR]"

msgctxt "#31409"
msgid "[B]SYSTEM INFO[/B][CR][CR][COLOR=midgrey]Summary · Storage · Network · Video · Hardware[/COLOR]"
msgstr "[B]SYSTEEM INFORMATIE[/B][CR][CR][COLOR=midgrey]Overzicht · Opslag · Netwerk · Video · Hardware[/COLOR]"

msgctxt "#31410"
msgid "[B]CONFIGURE TV SETTINGS[/B][CR][CR][COLOR=midgrey]Change fullscreen info · Manage EPG data settings[/COLOR]"
msgstr "[B]CONFIGUREER TV INSTELLINGEN[/B][CR][CR][COLOR=midgrey]verander volledig scherm info · Beheer EPG data instellingen[/COLOR]"

msgctxt "#31421"
msgid "[B]SELECT YOUR XBMC USER PROFILE[/B][CR][CR][COLOR=midgrey]to login and continue[/COLOR]"
msgstr "[B]SELECTEER UW XBMC GEBRUIKERS PROFIEL[/B][CR][CR][COLOR=midgrey]om in te loggen en door te gaan[/COLOR]"

